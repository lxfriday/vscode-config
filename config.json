{
  "typescript.tsdk": "/usr/local/lib/node_modules/typescript/lib",
  "editor.tabSize": 2,
  "editor.fontSize": 10,
  "editor.fontWeight": "200",
  "window.zoomLevel": 0,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Monokai",
